  <data name="SQL_FSM_PARTICIPANTS" xml:space="preserve">
    <value>-- Singel
select	p.FederationId,
			SUBSTRING(CAST(p.BirthDate AS char),1,10) AS BirthDate,
			P.FirstName,
			p.LastName,
			cl.Name AS CLUBNAME,
			ms.Title as MusicShort,
			mf.Title as MusicFree,
			CAST(crs.StartNumber AS char) as StartNoShort,
			CAST(crf.StartNumber AS char) as StartNoFree,
			c.Name as CategoryName
from		person p
			left outer join club cl on cl.ShortName=p.Club
			inner join single s on s.Person_Id=p.Id								-- Koppling från Person till CompetitorID
			inner join entry e on e.Competitor_Id=s.Competitor_Id_Pk			-- Koppling från singel till Tävling, länk till Category (även status??)
			inner join category c on c.Id=e.Category_Id							-- Klass via Entry
			inner join competitor cm on cm.Id=s.Competitor_Id_Pk				-- Koppling till musik
			left outer join music ms on ms.Id=cm.ShortMusik_Id					-- Musik från competitor
			left outer join music mf on mf.Id=cm.FreeMusik_Id					-- Musik från competitor
			left outer join competitorresult crs									-- Startnummer Kortprogram
				on crs.Entry_Id=e.Id
				and crs.Segment_Id IN (select Id from segment where SegmentType=0)
			left outer join competitorresult crf									-- Startnummer Friåkning
				on crf.Entry_Id=e.Id
				and crf.Segment_Id IN (select Id from segment where SegmentType=1)

UNION


/* Couple */
select	co.FederationId,
			NULL,
			CONCAT(pf.FirstName,' ',pf.LastName,' / ',pm.FirstName,' ',pm.LastName) AS FirstName,
			NULL AS LastName,
			NULL AS CLUBNAME,
			ms.Title as MusicShort,
			mf.Title as MusicFree,
			CAST(crs.StartNumber AS char) as StartNoShort,
			CAST(crf.StartNumber AS char) as StartNoFree,
			CONCAT(c.Name,
				CASE c.`Type`
					WHEN	2	THEN ' (Par)'
					WHEN	3	THEN ' (Isdans)'
					WHEN	4	THEN ' (Synkroniserad konståkning)'
				END) as CategoryName
from		couple co
			inner join person pf on pf.Id=co.PersonLady_Id
			inner join person pm on pm.Id=co.PersonMale_Id
			inner join entry e on e.Competitor_Id=co.Competitor_Id_Pk		-- Koppling från couple till Tävling, länk till Category (även status??)
			inner join category c on c.Id=e.Category_Id							-- Klass via Entry
			inner join competitor cm on cm.Id=co.Competitor_Id_Pk				-- Koppling till musik
			left outer join music ms on ms.Id=cm.ShortMusik_Id					-- Musik från competitor
			left outer join music mf on mf.Id=cm.FreeMusik_Id					-- Musik från competitor
			left outer join competitorresult crs									-- Startnummer Kortprogram
				on crs.Entry_Id=e.Id
				and crs.Segment_Id IN (select Id from segment where SegmentType=0)
			left outer join competitorresult crf									-- Startnummer Friåkning
				on crf.Entry_Id=e.Id
				and crf.Segment_Id IN (select Id from segment where SegmentType=1)</value>
